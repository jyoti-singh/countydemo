<h1>Listing countytypes</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @countytypes.each do |countytype| %>
      <tr>
        <td><%= countytype.type %></td>
        <td><%= link_to 'Show', countytype %></td>
        <td><%= link_to 'Edit', edit_countytype_path(countytype) %></td>
        <td><%= link_to 'Destroy', countytype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Countytype', new_countytype_path %>

